{
  "features": {
    "edit_prediction_provider": "zed"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "format_on_save": "on",
  "formatter": "language_server",
  "languages": {
    "Rust": {
      "edit_predictions_disabled_in": [
        "comment",
        "string"
      ]
    }
  },
  "base_keymap": "JetBrains",
  "ui_font_family": "Liberation Mono",
  "buffer_font_family": "Liberation Mono",
  "ui_font_size": 15,
  "buffer_font_size": 14.0,
  "current_line_highlight": "gutter",
  "toolbar": {
    "selections_menu": false,
    "breadcrumbs": false,
    "quick_actions": false
  },
  "outline_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "git_status": true
  },
  "gutter": {
    "code_actions": false,
    "folds": false
  },
  "file_icons": "true",
  "scrollbar": {
    "show": "never"
  },
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "show_background": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50
  },
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light Hard",
    "dark": "Catppuccin Espresso (Blur)"
  },
  "icon_theme": "Catppuccin Latte",
  "indent_guides": {
    "coloring": "fixed",
    "background_coloring": "disabled"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "shell": {
      "program": "/bin/bash"
    },
    "font_family": "Liberation Mono"
  },
  "file_types": {
    "C": [
      "cl",
      "cpp",
      "cu",
      "cuh"
    ]
  }
}
